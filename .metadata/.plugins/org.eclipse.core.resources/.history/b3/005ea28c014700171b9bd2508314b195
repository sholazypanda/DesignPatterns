
public class Person {
	String name;
	

	RemoteControl remoteControl;
	public Person(String name, RemoteControl remoteControl) {
		super();
		this.name = name;
		this.remoteControl = remoteControl;
	}
	public Person(String name) {
		super();
		this.name = name;
	}
	public static void main(String args[]){
		RemoteControl rm = new RemoteControl();
		Person person1 = new Person("Mary",rm);
		
		Projector projObj = new Projector("Horly");
		SurroundSoundSystem surrObj = new SurroundSoundSystem("Bose");
		TV tvObj = new TV("Sony");
		
	}
}
